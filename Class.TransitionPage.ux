<Page ux:Class="TransitionPage" Transition="None">
	<ResourceString Key="PageTitle" Value="{pagetitle}" />
    <EnteringAnimation>
        <Move X="1" RelativeTo="Size" Duration="0.4" Easing="BackOut" />
    </EnteringAnimation>
    <ExitingAnimation>
        <Move X="-1" RelativeTo="Size" Duration="0.4" Easing="BackOut" />
    </ExitingAnimation>
    <RemovingAnimation>
        <Move X="-1" RelativeTo="Size" Duration="0.4" Easing="BackOut" />
    </RemovingAnimation>
</Page>
